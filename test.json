{
  "name": "developer-portal-front",
  "description": "developer portal front frontend project",
  "license": "UNLICENSED",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">= 8.5.0"
  },
  "engineStrict": true,
  "browserslist": [
    "extends @polestar/browserslist-config/web3"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=webpack NODE_ENV=production webpack --config webpack.prod.js --colors --display-error-details",
    "cp-env": "devhouse-dotenv copy -e .env.example",
    "dev": "cross-env BABEL_ENV=webpack NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "eslint": "yarn devhouse-quality-check -c eslint --verbose",
    "postinstall": "yarn cp-env",
    "preprod:start": "yarn build",
    "prod:start": "npx serve -p 3000 -s build/public/web",
    "prequality-check": "yarn test:unit && yarn build",
    "quality-check": "devhouse-quality-check  --skip-lighthouse --skip-eslint",
    "quality-check:browser": "yarn quality-check --browser",
    "show-babel": "devhouse-config-utils show-config -c ./babel.config.js",
    "show-jestconfig-puppeteer": "devhouse-config-utils show-config -c ./jest-puppeteer.config.js",
    "show-jestconfig-unit": "cross-env TEST=unit devhouse-config-utils show-config -c ./jest.config.js",
    "show-jestconfig-e2e": "cross-env TEST=e2e devhouse-config-utils show-config -c ./jest.config.js",
    "show-quality-docs": "devhouse-quality-check --show-docs",
    "show-tsconfig": "devhouse-config-utils show-tsconfig -t tsconfig.json",
    "show-webpack-dev": "devhouse-config-utils show-config -c ./webpack.dev.js",
    "show-webpack-prod": "NODE_ENV=production devhouse-config-utils show-config -c ./webpack.prod.js",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:e2e": "yarn run build && cross-env BABEL_ENV=test TEST=e2e babel-node ./node_modules/jest/bin/jest.js tests/end-to-end",
    "test:e2e:debug": "cross-env DEBUG=true yarn run test:e2e",
    "test:unit": "cross-env BABEL_ENV=test TEST=unit babel-node ./node_modules/jest/bin/jest.js --coverage --config=jest.config.js",
    "test:unit:watch": "yarn run test:unit --watch"
  },
  "dependencies": {
    "@apollo/client": "3.2.4",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@loadable/component": "^5.13.2",
    "@polestar/browserslist-config": "^1.0.11",
    "@polestar/component-warehouse-react": "^79.0.1",
