    "@polestar/web3-core-react": "^26.5.0",
    "@sentry/react": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "@types/react-scroll": "^1.8.2",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "formik": "^2.2.6",
    "graphql": "^15.3.0",
    "react": "^16.8.0",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.2",
    "yup": "^0.32.9"
  },
